.catalog-list
    display: grid
    grid-template-columns: repeat(4, 1fr)
    gap: 55px

.catalog2-list
    display: grid
    grid-template-columns: repeat(3, 1fr)

@media (min-width: 1800px)
    .catalog2-list
        gap: 75px 86px

@media (max-width: 1800px)
    .catalog2-list
        gap: vh(75) vw(86)